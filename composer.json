{
  "name": "tourze/user-avatar-bundle",
  "description": "提供用户头像管理和处理功能的Symfony Bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-hash": "*",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/security-core": "^7.3",
    "symfony/service-contracts": "^3.6",
    "symfony/yaml": "^7.3",
    "tourze/symfony-dependency-service-loader": "1.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\UserAvatarBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\UserAvatarBundle\\Tests\\": "tests"
    }
  }
}